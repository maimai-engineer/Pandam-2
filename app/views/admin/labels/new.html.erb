<div class="container">

      <h2>レーベル新規作成</h2>
      <%= form_for(@label, url: admin_labels_path, method: :post) do |f| %>
        <div class="form-group">
          <label for="label">レーベル名</label>
          <%= f.text_field :label, id: "label", class: "form-control" %>
        </div>
        <%= f.submit "Create", class: "btn btn-success" %>
      <% end %>
      <%= link_to "Back", admin_labels_path %><br>

</div>
